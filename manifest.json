{
    "manifest_version": 3,
    "name": "Endless Browser Navigator",
    "version": "1.0",
    "description": "A Chrome extension that endlessly browses the internet by clicking on links.",
    "permissions": [
      "tabs",
      "scripting",
      "activeTab",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.jpeg",
        "48": "icons/icon48.jpeg",
        "128": "icons/icon128.jpeg"
      }
    },
    "icons": {
      "16": "icons/icon16.jpeg",
      "48": "icons/icon48.jpeg",
      "128": "icons/icon128.jpeg"
    },
    "host_permissions": [
      "<all_urls>"
    ]
  }
  